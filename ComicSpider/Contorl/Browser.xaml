<Window x:Class="ComicSpider.Browser"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Browser" Height="500" Width="600">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="35" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Grid>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="232*" />
				<ColumnDefinition Width="60" />
			</Grid.ColumnDefinitions>
			<TextBox x:Name="txt_navibar" Margin="5"></TextBox>
			<Button Margin="5" Grid.Column="1" Click="Go">Go</Button>
		</Grid>
		<WebBrowser x:Name="browser" Grid.Row="1"></WebBrowser>
	</Grid>
</Window>
